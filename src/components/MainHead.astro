---
import "../styles/global.css";
const {
  title = "George Vagdas: Personal Site",
  description = "The personal site of George Vagdas",
} = Astro.props;
---

<meta charset="UTF-8" />
<meta name="description" property="og:description" content={description} />
<meta name="viewport" content="width=device-width" />
<meta name="generator" content={Astro.generator} />
<title>{title}</title>

<link rel="icon" type="image/svg+xml" href="/favicon.webp" />
<!-- Preload for Silka Regular font -->
<link
  rel="preload"
  href="/fonts/silka/silka-regular-webfont.woff2"
  as="font"
  crossorigin
/>
<link
  rel="preload"
  href="/fonts/silka/silka-regular-webfont.woff"
  as="font"
  crossorigin
/>
<link
  rel="preload"
  href="/fonts/silka/silka-regular-webfont.ttf"
  as="font"
  crossorigin
/>

<!-- Preload for Moranga Bold font -->
<link
  rel="preload"
  href="/fonts/moranga/moranga-bold.otf"
  as="font"
  crossorigin
/>

<!-- Add these in your HTML file (e.g., index.html) -->
<link
  href="https://api.mapbox.com/mapbox-gl-js/v2.6.1/mapbox-gl.css"
  rel="stylesheet"
/>
<script src="https://api.mapbox.com/mapbox-gl-js/v2.6.1/mapbox-gl.js"></script>

<script src="https://kit.fontawesome.com/a0c9def029.js" crossorigin="anonymous"
></script>
<script
  async
  src="https://analytics.eu.umami.is/script.js"
  data-website-id="46265325-49af-4aa8-902c-ba4ed597a978"></script>
